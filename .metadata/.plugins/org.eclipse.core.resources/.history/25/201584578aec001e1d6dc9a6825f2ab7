	public static void main(String[] args) {
		ContaBancaria c1 = new ContaBancaria();
		c1.agencia = 341;
		c1.banco = 341;
		c1.numeroConta = 12345;
		c1.saldo = -10;
		ContaBancaria c2 = c1;
		System.out.println(c1.saldo);
		
		
		
	}

}
